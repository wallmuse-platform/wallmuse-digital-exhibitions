# WallMuse WebPlayer Environment Configuration
# Copy this file to .env or .env.development and fill in your values

# WebSocket endpoint for real-time communication
REACT_APP_ROOTWS=wss://manager.wallmuse.com:8444/wallmuse/ws_commands

# REST API endpoint
REACT_APP_ROOTURL=https://manager.wallmuse.com:8444/wallmuse/ws/

# Test credentials (optional - for development/testing only)
# WARNING: Never commit real credentials to version control
REACT_APP_TESTLOGIN=
REACT_APP_TESTPWD=
REACT_APP_TESTTOKEN=

# For local development, use these instead:
# REACT_APP_ROOTWS=ws://localhost:8080/wallmuse/ws_commands
# REACT_APP_ROOTURL=http://localhost:8080/wallmuse/ws/
